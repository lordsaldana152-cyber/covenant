{
  "version": 2,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*).(mp4|mp3|wav|ogg|png|jpg|jpeg|gif|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/new.html"
    }
  ]
}